{
  "name": "network-canvas-server",
  "version": "4.0.0-alpha.1",
  "productName": "Network Canvas Server",
  "description": "A tool for storing, analyzing, and exporting Network Canvas interview data.",
  "private": true,
  "author": "Complex Data Collective",
  "engines": {
    "node": "8.9.3",
    "npm": "5.3.0"
  },
  "scripts": {
    "start": "cross-env BUILD_TARGET=web node scripts/start.js",
    "start:electron-renderer": "node scripts/start.js",
    "prestart:electron": "node scripts/build-electron-dev.js && node scripts/build-api-spec.js",
    "start:electron": "cross-env NODE_ENV=development WEBPACK_DEV_SERVER_PORT=3000 electron electron-dev/",
    "build": "node scripts/build.js",
    "server:dev": "node ./scripts/server-starter",
    "lint": "eslint src",
    "lint-sass": "sass-lint -vc .sass-lint.yml",
    "test": "node scripts/test.js --env=jsdom",
    "test-update-snapshots": "node scripts/test.js --env=jsdom --updateSnapshot",
    "build-docs": "jsdoc src -r -d docs -c ./jsdoc.conf.json --verbose --readme ./README.md",
    "electron": "electron ./www",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist:mac": "electron-builder -m",
    "dist:linux": "electron-builder -l",
    "dist:win": "electron-builder -w",
    "dist:all": "electron-builder -mlw",
    "generate-icons": "node scripts/generate-app-icons.js"
  },
  "repository": {
    "type": "git"
  },
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "^7.1.4",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-app": "^3.0.2",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.10",
    "dotenv": "4.0.0",
    "electron": "^2.0.0-beta.5",
    "electron-builder": "^20.5.1",
    "electron-log": "^2.2.7",
    "electron-packager": "^11.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "es6-promise-plugin": "^4.1.0",
    "eslint": "^4.1.1",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "^0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "i": "^0.3.6",
    "icon-gen": "^1.2.1",
    "istanbul-lib-coverage": "^1.2.0",
    "jest": "^22.4.3",
    "jest-fetch-mock": "^1.4.2",
    "jsdoc": "^3.4.3",
    "jsdoc-babel": "^0.3.0",
    "json-loader": "^0.5.7",
    "network-canvas-ui": "git+https://git@github.com/codaco/Network-Canvas-UI.git#1.13.1",
    "node-sass": "^4.5.2",
    "postcss-loader": "^2.1.1",
    "promise": "7.1.1",
    "react-dev-utils": "^5.0.1",
    "react-scripts": "^1.1.4",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.7",
    "sass-svg": "^1.1.0",
    "style-loader": "^0.13.2",
    "swagger-jsdoc": "^1.9.7",
    "url-loader": "^0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "^1.3.1",
    "whatwg-fetch": "2.0.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "detect-port": "^1.2.2",
    "jszip": "^3.1.5",
    "libsodium-wrappers": "^0.7.3",
    "mdns": "^2.3.4",
    "nedb": "^1.8.0",
    "private-socket": "git+https://github.com/codaco/PrivateSocket.git",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^4.4.5",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^16.3.0",
    "react-transition-group": "^2.2.1",
    "recharts": "^1.0.0-beta.10",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-persist": "^4.3.1",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.0",
    "restify": "^6.3.4",
    "restify-cors-middleware": "^1.1.0",
    "uuid": "^3.2.1"
  },
  "build": {
    "extends": null,
    "appId": "NetworkCanvasServer",
    "directories": {
      "app": "./www",
      "buildResources": "build-resources",
      "output": "release-builds"
    },
    "mac": {
      "category": "public.app-category.education"
    },
    "win": {
      "target": "nsis"
    }
  },
  "homepage": ".",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/polyfills.js",
      "<rootDir>/src/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts|release-builds)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
