{
  "name": "network-canvas-server",
  "version": "4.0.0-alpha.2",
  "productName": "Network Canvas Server",
  "description": "A tool for storing, analyzing, and exporting Network Canvas interview data.",
  "private": true,
  "author": "Complex Data Collective",
  "engines": {
    "node": "8.9.3",
    "npm": "6.1.0"
  },
  "scripts": {
    "start": "cross-env BUILD_TARGET=web node scripts/start.js",
    "start:electron": "node scripts/start.js",
    "preelectron:dev": "node scripts/build-electron-dev.js && node scripts/build-api-spec.js",
    "electron:dev": "cross-env NODE_ENV=development WEBPACK_DEV_SERVER_PORT=3000 electron electron-dev/",
    "build": "node scripts/build.js",
    "server:dev": "NODE_ENV=development node ./scripts/server-starter",
    "lint": "eslint src",
    "sass-lint": "sass-lint -vc .sass-lint.yml",
    "test": "node scripts/test.js --env=jsdom",
    "test-update-snapshots": "node scripts/test.js --env=jsdom --updateSnapshot",
    "build-docs": "jsdoc src -r -d docs -c ./jsdoc.conf.json --verbose --readme ./README.md",
    "electron": "electron ./www",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist:mac": "electron-builder -m",
    "dist:linux": "electron-builder -l",
    "dist:win": "electron-builder -w",
    "dist:all": "electron-builder -mlw",
    "generate-icons": "node scripts/generate-app-icons.js",
    "preflight": "npm run lint && npm run sass-lint -- --max-warnings 0 && npm run build-docs && node scripts/build-api-spec && npm test -- --coverage"
  },
  "repository": {
    "type": "git"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.5",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.5",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-app": "^3.1.2",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "^2.4.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "electron": "^2.0.4",
    "electron-builder": "^20.19.2",
    "electron-log": "^2.2.16",
    "electron-packager": "^12.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.1.1",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "fs-extra": "^6.0.1",
    "html-webpack-plugin": "^3.2.0",
    "icon-gen": "^1.2.1",
    "istanbul-lib-coverage": "^1.2.0",
    "jest": "^23.4.0",
    "jest-fetch-mock": "^1.6.5",
    "jsdoc": "^3.4.3",
    "jsdoc-babel": "^0.4.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "postcss-loader": "^2.1.6",
    "react-dev-utils": "^6.0.0-next.3e165448",
    "react-scripts": "^1.1.4",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "swagger-jsdoc": "^1.9.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.0",
    "webpack-dev-server": "^3.1.4",
    "webpack-manifest-plugin": "^2.0.3",
    "whatwg-fetch": "2.0.2"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "detect-port": "^1.2.3",
    "jszip": "^3.1.5",
    "libsodium-wrappers": "^0.7.3",
    "mdns": "^2.4.0",
    "nedb": "^1.8.0",
    "private-socket": "git+https://github.com/codaco/PrivateSocket.git",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^4.4.9",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.4.1",
    "react-transition-group": "^2.4.0",
    "recharts": "^1.0.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-persist": "^4.3.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.0",
    "restify": "^7.2.1",
    "restify-cors-middleware": "^1.1.1",
    "uuid": "^3.3.2"
  },
  "browserslist": [
    "Chrome 61"
  ],
  "build": {
    "extends": null,
    "appId": "NetworkCanvasServer",
    "directories": {
      "app": "./www",
      "buildResources": "build-resources",
      "output": "release-builds"
    },
    "mac": {
      "category": "public.app-category.education"
    },
    "win": {
      "target": "nsis"
    }
  },
  "homepage": ".",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/src/renderer/ui/**",
      "!**/dummy_data.js"
    ],
    "modulePathIgnorePatterns": [
      "electron-dev/",
      "src/main/package.json",
      "www/"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/polyfills.js",
      "<rootDir>/config/jest/setupTestEnv.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/setupTestFramework.js",
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts|release-builds)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
