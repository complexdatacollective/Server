{
  "name": "network-canvas-server",
  "version": "4.0.0",
  "productName": "Network Canvas Server",
  "description": "A tool for storing, analyzing, and exporting Network Canvas interview data.",
  "private": true,
  "author": "Complex Data Collective",
  "engines": {
    "node": "8.2.1",
    "npm": "5.3.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "server:dev": "node ./src/main/server-starter",
    "server": "node ./www/server-starter",
    "lint": "eslint src",
    "test": "NODE_ENV=test node scripts/test.js --env=jsdom",
    "test-update-snapshots": "node scripts/test.js --env=jsdom --updateSnapshot",
    "build-docs": "jsdoc src -r -d docs-build -c ./jsdoc.conf.json --verbose",
    "electron": "electron ./www",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist:mac": "electron-builder -m",
    "dist:linux": "electron-builder -l",
    "dist:win": "electron-builder -w",
    "dist:all": "electron-builder -mlw"
  },
  "repository": {
    "type": "git"
  },
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-app": "^3.0.2",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.5.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.28.4",
    "detect-port": "1.1.0",
    "dotenv": "4.0.0",
    "electron": "^1.8.0",
    "electron-builder": "^19.56.2",
    "electron-installer-dmg": "^0.2.0",
    "electron-log": "^2.2.7",
    "electron-packager": "^8.5.2",
    "electron-to-chromium": "^1.3.21",
    "electron-updater": "^2.9.3",
    "electron-winstaller": "^2.5.2",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "es6-promise-plugin": "^4.1.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-react-app": "^0.6.2",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "^0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "0.17.3",
    "icon-gen": "^1.0.7",
    "jest": "20.0.4",
    "jsdoc": "^3.4.3",
    "jsdoc-babel": "^0.3.0",
    "json-loader": "0.5.4",
    "mock-socket": "^7.0.0",
    "network-canvas-ui": "git+https://git@github.com/codaco/Network-Canvas-UI.git",
    "node-sass": "^4.5.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "7.1.1",
    "react-addons-test-utils": "^15.5.1",
    "react-dev-utils": "^4.0.1",
    "react-scripts": "^1.0.13",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.6",
    "sass-svg": "^1.1.0",
    "style-loader": "0.13.1",
    "url-loader": "^0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "^1.3.1",
    "whatwg-fetch": "2.0.2"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "change-case": "^3.0.1",
    "classnames": "^2.2.5",
    "cote": "^0.15.1",
    "create-react-class": "^15.5.3",
    "history": "^4.5.1",
    "ip": "^1.1.5",
    "lodash": "^4.17.4",
    "menubar": "^5.2.3",
    "nedb": "^1.8.0",
    "private-socket": "git+https://github.com/codaco/PrivateSocket.git",
    "prop-types": "^15.5.8",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-icons": "^2.2.3",
    "react-redux": "^4.4.5",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^15.5.4",
    "react-transform-hmr": "^1.0.4",
    "react-transition-group": "^1.1.3",
    "recharts": "^1.0.0-alpha.2",
    "redux": "^3.6.0",
    "redux-form": "^6.4.3",
    "redux-logger": "^2.7.4",
    "redux-observable": "^0.14.1",
    "redux-persist": "^4.3.1",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.0",
    "rxjs": "^5.4.0",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.4",
    "upgrade": "^1.1.0"
  },
  "build": {
    "extends": null,
    "appId": "NetworkCanvasServer",
    "directories": {
      "app": "./www",
      "buildResources": "build-resources",
      "output": "release-builds"
    },
    "mac": {
      "category": "public.app-category.education"
    },
    "win": {
      "target": "nsis"
    }
  },
  "homepage": ".",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/polyfills.js",
      "<rootDir>/src/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts|release-builds)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
