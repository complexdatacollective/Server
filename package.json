{
  "name": "network-canvas-server",
  "version": "4.0.0-alpha.11",
  "productName": "Network Canvas Server",
  "description": "A tool for storing, analyzing, and exporting Network Canvas interview data.",
  "private": true,
  "author": "Complex Data Collective",
  "engines": {
    "node": "8.9.3",
    "npm": "6.1.0"
  },
  "scripts": {
    "start": "cross-env PORT=4000 BUILD_TARGET=web node scripts/start.js",
    "start:electron": "cross-env PORT=4000 node scripts/start.js",
    "preelectron:dev": "node scripts/build-electron-dev.js && node scripts/build-api-spec.js",
    "electron:dev": "cross-env NODE_ENV=development WEBPACK_DEV_SERVER_PORT=4000 electron electron-dev/",
    "build": "node scripts/build.js",
    "lint": "eslint src",
    "sass-lint": "sass-lint -vc .sass-lint.yml",
    "test": "NODE_TLS_REJECT_UNAUTHORIZED=0 node scripts/test.js --env=jsdom",
    "test-update-snapshots": "node scripts/test.js --env=jsdom --updateSnapshot",
    "build-docs": "jsdoc src -r -d docs -c ./jsdoc.conf.json --verbose --readme ./README.md",
    "electron": "electron ./www",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "predist:mac": "node scripts/check-env.js darwin",
    "predist:linux": "node scripts/check-env.js linux",
    "predist:win": "node scripts/check-env.js win32",
    "dist:mac": "npm run build && electron-builder -m --publish=always",
    "dist:linux": "npm run build && electron-builder -l --publish=always",
    "dist:win": "npm run build && electron-builder -w --publish=always",
    "generate-icons": "node scripts/generate-app-icons.js",
    "preflight": "npm run lint -- --max-warnings 0 && npm run sass-lint -- --max-warnings 0 && npm run build-docs && node scripts/build-api-spec && npm test -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codaco/Server.git"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.5",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.5",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-app": "^3.1.2",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "^2.4.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "electron": "^2.0.11",
    "electron-builder": "^20.27.1",
    "electron-log": "^2.2.16",
    "electron-packager": "^12.1.0",
    "electron-updater": "~3.0.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.1.1",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "^7.11.0",
    "file-loader": "^1.1.11",
    "fs-extra": "^6.0.1",
    "html-webpack-plugin": "^3.2.0",
    "icon-gen": "^1.2.1",
    "istanbul-lib-coverage": "^1.2.0",
    "jest": "^23.4.0",
    "jest-fetch-mock": "^1.6.5",
    "jsdoc": "^3.4.3",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.3",
    "postcss-loader": "^2.1.6",
    "react-dev-utils": "^6.0.0-next.a671462c",
    "react-scripts": "^2.0.0-next.a671462c",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "swagger-jsdoc": "^3.0.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.0",
    "webpack-dev-server": "^3.1.4",
    "webpack-manifest-plugin": "^2.0.3",
    "whatwg-fetch": "2.0.2"
  },
  "dependencies": {
    "animejs": "^2.2.0",
    "archiver": "^2.1.1",
    "classnames": "^2.2.6",
    "detect-port": "^1.2.3",
    "jszip": "^3.1.5",
    "libsodium-wrappers": "^0.7.3",
    "lodash": "^4.17.11",
    "mdns": "^2.4.0",
    "nedb": "^1.8.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-error-overlay": "^5.0.0-next.3e165448",
    "react-redux": "^4.4.9",
    "react-router-dom": "^4.3.1",
    "react-sortable-hoc": "^0.6.8",
    "react-test-renderer": "^16.4.1",
    "react-transition-group": "^2.5.0",
    "recharts": "^1.0.1",
    "recompose": "^0.26.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-persist": "^4.3.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.0",
    "restify": "^7.2.1",
    "restify-cors-middleware": "^1.1.1",
    "selfsigned": "^1.10.3",
    "thread-loader": "^1.1.5",
    "uuid": "^3.3.2",
    "xmldom": "^0.1.27"
  },
  "browserslist": [
    "Chrome 61"
  ],
  "build": {
    "extends": null,
    "appId": "NetworkCanvasServer",
    "directories": {
      "app": "./www",
      "buildResources": "build-resources",
      "output": "release-builds"
    },
    "mac": {
      "category": "public.app-category.education"
    },
    "win": {
      "target": "nsis",
      "certificateSubjectName": "Complex Data Collective"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "codaco",
        "repo": "Server"
      }
    ]
  },
  "homepage": ".",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/main/components/loadDevToolsExtensions.js",
      "!src/renderer/ui/**",
      "!src/main/utils/network-query/**",
      "!src/renderer/components/Filter/**",
      "!src/renderer/components/Transitions/(Appear|Drawer)*",
      "!**/dummy_data.js"
    ],
    "modulePathIgnorePatterns": [
      "electron-dev/",
      "src/main/package.json",
      "www/"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/automock.js",
      "<rootDir>/config/jest/polyfills.js",
      "<rootDir>/config/jest/setupTestEnv.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/setupTestFramework.js",
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts|release-builds)[/\\\\]",
      "<rootDir>/src/main/utils/network-query",
      "<rootDir>/src/renderer/ui"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
