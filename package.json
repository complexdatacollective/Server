{
    "name": "network-canvas-server",
    "version": "5.1.0",
    "productName": "Network Canvas Server",
    "description": "A tool for storing, analyzing, and exporting Network Canvas interview data.",
    "private": true,
    "author": "Complex Data Collective",
    "engines": {
        "node": "12.14.1",
        "npm": "6.13.4"
    },
    "scripts": {
        "start": "cross-env PORT=4000 BUILD_TARGET=web node scripts/start.js",
        "start:electron": "cross-env PORT=4000 node scripts/start.js",
        "preelectron:dev": "node scripts/build-electron-dev.js && node scripts/build-api-spec.js",
        "electron:dev": "cross-env NODE_ENV=development WEBPACK_DEV_SERVER_PORT=4000 electron electron-dev/",
        "build": "node scripts/build.js",
        "lint": "eslint src",
        "sass-lint": "sass-lint -vc .sass-lint.yml",
        "test": "NODE_TLS_REJECT_UNAUTHORIZED=0 node scripts/test.js --env=jsdom",
        "test-update-snapshots": "node scripts/test.js --env=jsdom --updateSnapshot",
        "build-docs": "jsdoc src -r -d docs -c ./jsdoc.conf.json --verbose --readme ./README.md",
        "electron": "electron ./www",
        "pack": "electron-builder --dir",
        "dist": "electron-builder",
        "bump": "node scripts/bump.js",
        "predist:mac": "node scripts/check-env.js darwin",
        "predist:linux": "node scripts/check-env.js linux",
        "predist:win": "node scripts/check-env.js win32",
        "dist:mac": "npm run build && electron-builder -m --publish=always",
        "dist:linux": "npm run build && electron-builder -l --publish=always",
        "dist:win": "npm run build && electron-builder -w --publish=always",
        "generate-icons": "node scripts/generate-app-icons.js",
        "preflight": "npm run lint -- --max-warnings 0 && npm run sass-lint -- --max-warnings 0 && npm run build-docs && node scripts/build-api-spec && npm test -- --coverage"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/codaco/Server.git"
    },
    "devDependencies": {
        "@babel/core": "^7.10.4",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/plugin-proposal-json-strings": "^7.10.4",
        "@babel/plugin-syntax-import-meta": "^7.10.4",
        "@babel/plugin-transform-destructuring": "^7.10.4",
        "@babel/plugin-transform-regenerator": "^7.10.4",
        "@babel/plugin-transform-runtime": "^7.10.4",
        "@babel/preset-env": "^7.10.4",
        "@babel/preset-react": "^7.10.4",
        "@codaco/ui": "^3.4.1",
        "autoprefixer": "^8.6.5",
        "babel-eslint": "^9.0.0",
        "babel-jest": "^25.5.1",
        "babel-loader": "^8.1.0",
        "case-sensitive-paths-webpack-plugin": "^2.1.2",
        "chalk": "^2.4.1",
        "cross-env": "^5.2.0",
        "css-loader": "^1.0.0",
        "dotenv": "^6.0.0",
        "electron": "^9.1.0",
        "electron-builder": "^22.7.0",
        "electron-devtools-installer": "^2.2.4",
        "electron-log": "^4.2.2",
        "electron-notarize": "^1.0.0",
        "electron-updater": "^4.3.1",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "enzyme-to-json": "^3.5.0",
        "eslint": "^4.1.1",
        "eslint-config-airbnb": "^15.0.1",
        "eslint-config-react-app": "^2.1.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-loader": "^2.2.1",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "5.1.1",
        "eslint-plugin-react": "^7.20.3",
        "faker": "^4.1.0",
        "file-loader": "^1.1.11",
        "fs-extra": "^6.0.1",
        "html-webpack-plugin": "^4.3.0",
        "icon-gen": "^1.2.1",
        "istanbul-lib-coverage": "^1.2.0",
        "jest": "^25.5.4",
        "jest-fetch-mock": "^1.6.5",
        "jsdoc": "^3.6.4",
        "json-loader": "^0.5.7",
        "mini-css-extract-plugin": "^0.8.0",
        "node-sass": "^4.14.1",
        "postcss-loader": "^2.1.6",
        "react-dev-utils": "^6.1.1",
        "rosie": "^2.0.1",
        "react-draggable": "^4.4.3",
        "react-virtualized": "^9.21.2",
        "redux-logger": "^3.0.6",
        "redux-persist": "^6.0.0",
        "regenerator-runtime": "^0.13.5",
        "sass-lint": "^1.12.1",
        "sass-loader": "^7.0.3",
        "sass-svg": "^1.0.1",
        "style-loader": "^0.21.0",
        "swagger-jsdoc": "^3.7.0",
        "url-loader": "^1.0.1",
        "webpack": "^4.43.0",
        "webpack-dev-server": "^3.11.0",
        "webpack-manifest-plugin": "^2.0.4",
        "whatwg-fetch": "2.0.2"
    },
    "dependencies": {
        "@babel/runtime": "^7.10.4",
        "animejs": "^2.2.0",
        "archiver": "^2.1.1",
        "big-integer": "^1.6.43",
        "classnames": "^2.2.6",
        "detect-port": "^1.2.3",
        "duplex-child-process": "^1.0.0",
        "electron-ipc-stream": "^0.2.0",
        "framer-motion": "^1.11.0",
        "luxon": "^1.22.0",
        "jszip": "^3.5.0",
        "libsodium-wrappers": "^0.7.3",
        "lodash": "^4.17.19",
        "mdns": "^2.5.1",
        "mississippi": "^4.0.0",
        "nedb": "^1.8.0",
        "prop-types": "^15.6.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-error-overlay": "^5.0.0-next.3e165448",
        "react-redux": "^4.4.9",
        "react-router-dom": "^4.3.1",
        "react-sortable-hoc": "^0.6.8",
        "react-test-renderer": "^16.13.1",
        "react-transition-group": "^2.5.0",
        "recharts": "^1.0.1",
        "recompose": "^0.26.0",
        "redux": "^3.6.0",
        "redux-actions": "^2.6.5",
        "redux-logger": "^2.7.4",
        "redux-persist": "^4.3.1",
        "redux-thunk": "^2.3.0",
        "reselect": "^3.0.0",
        "restify": "^7.2.1",
        "restify-cors-middleware": "^1.1.1",
        "selfsigned": "^1.10.7",
        "split": "^1.0.1",
        "thread-loader": "^1.1.5",
        "uuid": "^3.3.2",
        "xmldom": "^0.1.27"
    },
    "browserslist": [
        "Chrome 61"
    ],
    "build": {
        "extends": null,
        "appId": "NetworkCanvasServer",
        "afterSign": "./build-resources/scripts/afterSignHook.js",
        "directories": {
            "app": "./www",
            "buildResources": "build-resources",
            "output": "release-builds"
        },
        "mac": {
            "category": "public.app-category.education",
            "hardenedRuntime": true,
            "entitlements": "./build-resources/entitlements.mac.inherit.plist"
        },
        "win": {
            "target": "nsis",
            "certificateSha1": "512E5573B1961B0ADCAFB21288935F8A3E6E656F"
        },
        "publish": [
            {
                "provider": "github",
                "owner": "codaco",
                "repo": "Server"
            }
        ]
    },
    "homepage": ".",
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}",
            "!src/main/components/loadDevToolsExtensions.js",
            "!src/renderer/ui/**",
            "!src/main/utils/network-exporters/**",
            "!src/main/utils/network-query/**",
            "!src/renderer/components/Filter/**",
            "!src/renderer/components/Transitions/(Appear|Drawer)*",
            "!**/dummy_data.js"
        ],
        "modulePathIgnorePatterns": [
            "electron-dev/",
            "src/main/package.json",
            "www/"
        ],
        "setupFiles": [
            "<rootDir>/config/polyfills.js",
            "<rootDir>/config/jest/automock.js",
            "<rootDir>/config/jest/polyfills.js",
            "<rootDir>/config/jest/setupTestEnv.js"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/config/jest/setupTestFramework.js"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "testPathIgnorePatterns": [
            "<rootDir>[/\\\\](build|docs|node_modules|scripts|release-builds)[/\\\\]",
            "<rootDir>/src/main/utils/network-exporters",
            "<rootDir>/src/main/utils/network-query",
            "<rootDir>/src/renderer/ui"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "testRegex": "(/__tests__/.*|(\\.|/))(\\.test|-test)\\.[jt]sx?$",
        "transformIgnorePatterns": [
            "/node_modules/(?!@codaco/ui).+\\.js$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "%components/(.*)$": "<rootDir>/src/renderer/components/$1",
            "%containers/(.*)$": "<rootDir>/src/renderer/containers/$1",
            "%utils/(.*)$": "<rootDir>/src/renderer/utils/$1",
            "%modules/(.*)$": "<rootDir>/src/renderer/ducks/modules/$1",
            "%main/(.*)$": "<rootDir>/src/main/$1",
            "%renderer/(.*)$": "<rootDir>/src/renderer/$1"
        }
    },
    "eslintConfig": {
        "extends": "react-app"
    }
}
